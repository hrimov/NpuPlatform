{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgreSQL": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME};Username=${DB_USER};Password=${DB_PASSWORD};",
    "BlobStorage": "${BLOB_STORAGE_CONNECTION_STRING}"
  },
  "Jwt": {
    "Secret": "${JWT_SECRET_KEY}"
  },
  "AzureStorage": {
    "ContainerName": "${AZURE_STORAGE_CONTAINER_NAME}",
    "BaseUrl": "${AZURE_STORAGE_BASE_URL}"
  }
}
